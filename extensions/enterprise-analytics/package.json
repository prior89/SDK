{
  "name": "@locklearn/enterprise-analytics-extension",
  "version": "1.0.0",
  "description": "LockLearn SDK 확장 모듈: 기업용 학습 분석 및 사용기록 기반 인사이트",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "locklearn",
    "enterprise",
    "analytics",
    "learning",
    "business-intelligence",
    "usage-tracking",
    "ai-insights"
  ],
  "author": "LockLearn Enterprise Team",
  "license": "Commercial",
  "peerDependencies": {
    "@locklearn/partner-sdk": "^2.0.0"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.0.0",
    "rollup": "^4.9.0",
    "typescript": "^5.3.0"
  }
}
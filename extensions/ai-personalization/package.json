{
  "name": "@locklearn/ai-personalization-extension",
  "version": "1.0.0",
  "description": "LockLearn SDK 확장 모듈: AI 기반 개인화 학습 및 하이브리드 아키텍처",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "locklearn",
    "ai",
    "personalization",
    "machine-learning",
    "tensorflow-lite",
    "on-device-ai",
    "hybrid-architecture"
  ],
  "author": "LockLearn AI Team",
  "license": "Commercial",
  "peerDependencies": {
    "@locklearn/partner-sdk": "^2.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-react-native": "^0.8.0",
    "ml-matrix": "^6.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.0.0",
    "rollup": "^4.9.0",
    "typescript": "^5.3.0"
  }
}
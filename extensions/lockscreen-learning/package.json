{
  "name": "@locklearn/lockscreen-learning-extension",
  "version": "1.0.0",
  "description": "LockLearn SDK 확장 모듈: 잠금화면 학습 및 마이크로러닝 (특허 구현)",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "locklearn",
    "lockscreen",
    "widget",
    "microlearning",
    "android-16",
    "ios-live-activities",
    "patent-implementation"
  ],
  "author": "LockLearn Mobile Team",
  "license": "Commercial",
  "peerDependencies": {
    "@locklearn/partner-sdk": "^2.0.0"
  },
  "dependencies": {
    "react-native-keyguard": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.0.0", 
    "rollup": "^4.9.0",
    "typescript": "^5.3.0"
  }
}